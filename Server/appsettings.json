{
  "Kestrel": {
    "Endpoints": {
      "MyHttpEndpoint": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GuacamoleSharp": {},
  "Guacd": {
    "Hostname": "localhost",
    "Port": 4822
  },
  "Client": {
    "DefaultArguments": {
      "rdp": {
        "args": "connect",
        "port": 3389,
        "width": 1024,
        "height": 768,
        "dpi": 96,
        "security": "any",
        "ignore-cert": "true"
      },
      "vnc": {
        "args": "connect",
        "port": 5900,
        "width": 1024,
        "height": 768,
        "dpi": 96
      },
      "ssh": {
        "args": "connect",
        "port": 22,
        "width": 1024,
        "height": 768,
        "dpi": 96
      },
      "telnet": {
        "args": "connect",
        "port": 23,
        "width": 1024,
        "height": 768,
        "dpi": 96
      }
    }
  }
}
